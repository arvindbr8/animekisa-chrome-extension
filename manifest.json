{
    "name": "animekisa-ext",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://animekisa.tv/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Animekisa-ext",
        "default_icon": "icon.png"
    }
}